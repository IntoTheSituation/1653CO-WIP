        var i = {};
        for (var s = 0; s < r.length; s++) {
            i[r[s]] = true
        }
        var o = function(e) {
            this.element = e
        };
        o.prototype = n;
        e.fn[t] = function() {
            var n = arguments;
            var r = this;
            this.each(function() {
